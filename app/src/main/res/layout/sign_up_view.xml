<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/sign_up_password"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView"
        android:layout_width="75dp"
        android:layout_height="25dp"
        android:layout_marginLeft="29dp"
        android:layout_marginStart="31dp"
        android:text="FirstName "
        android:layout_marginTop="26dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:id="@+id/sign_up_first_Name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView"
        android:layout_marginLeft="27dp"
        android:layout_marginStart="27dp"
        android:layout_toEndOf="@+id/textView"
        android:layout_toRightOf="@+id/textView"
        android:ems="10"
        android:inputType="textPersonName" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="75dp"
        android:layout_height="25dp"
        android:layout_alignLeft="@+id/textView"
        android:layout_alignStart="@+id/textView"
        android:layout_below="@+id/sign_up_first_Name"
        android:layout_marginTop="33dp"
        android:layout_toLeftOf="@+id/sign_up_first_Name"
        android:layout_toStartOf="@+id/sign_up_first_Name"
        android:text="LastName" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="75dp"
        android:layout_height="25dp"
        android:layout_alignLeft="@+id/textView3"
        android:layout_alignStart="@+id/textView3"
        android:layout_below="@+id/textView3"
        android:layout_marginTop="29dp"
        android:text="Profession" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="75dp"
        android:layout_height="25dp"
        android:layout_alignEnd="@+id/textView4"
        android:layout_alignLeft="@+id/textView4"
        android:layout_alignRight="@+id/textView4"
        android:layout_alignStart="@+id/textView4"
        android:layout_below="@+id/textView4"
        android:layout_marginTop="33dp"
        android:text="City" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="75dp"
        android:layout_height="35dp"
        android:layout_alignLeft="@+id/textView5"
        android:layout_alignStart="@+id/textView5"
        android:layout_below="@+id/textView5"
        android:layout_marginTop="27dp"
        android:text="Email Address" />

    <EditText
        android:id="@+id/sign_up_last_Name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView4"
        android:layout_alignLeft="@+id/sign_up_first_Name"
        android:layout_alignStart="@+id/sign_up_first_Name"
        android:ems="10"
        android:inputType="textNoSuggestions|textPersonName" />

    <EditText
        android:id="@+id/sign_up_profession"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView5"
        android:layout_alignLeft="@+id/sign_up_last_Name"
        android:layout_alignStart="@+id/sign_up_last_Name"
        android:ems="10"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/sign_up_city"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView6"
        android:layout_alignLeft="@+id/sign_up_profession"
        android:layout_alignStart="@+id/sign_up_profession"
        android:ems="10"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/sign_up_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView6"
        android:layout_alignLeft="@+id/sign_up_city"
        android:layout_alignStart="@+id/sign_up_city"
        android:ems="10"
        android:inputType="textEmailAddress" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="75dp"
        android:layout_height="35dp"
        android:layout_alignLeft="@+id/textView6"
        android:layout_alignStart="@+id/textView6"
        android:layout_below="@+id/textView6"
        android:layout_marginTop="21dp"
        android:text="Upload Picture" />

    <Button
        android:id="@+id/sign_up_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sign Up"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="75dp"
        android:layout_height="25dp"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10"
        android:layout_below="@+id/textView10"
        android:layout_marginTop="31dp"
        android:text="Contact" />

    <EditText
        android:id="@+id/sign_up_contact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView11"
        android:layout_alignStart="@+id/sign_up_take_picture"
        android:ems="10"
        android:inputType="number" />

    <ImageButton
        android:id="@+id/sign_up_take_picture"
        android:layout_width="125dp"
        android:layout_height="35dp"
        android:layout_above="@+id/textView11"
        android:layout_alignEnd="@+id/sign_up_email"
        android:layout_alignStart="@+id/sign_up_email"
        android:layout_alignTop="@+id/textView10"
        app:srcCompat="@drawable/images" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="75dp"
        android:layout_height="25dp"
        android:layout_alignStart="@+id/textView11"
        android:layout_below="@+id/textView11"
        android:layout_marginTop="12dp"
        android:text="Street" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView8"
        android:layout_alignStart="@+id/sign_up_contact"
        android:layout_below="@+id/sign_up_contact"
        android:ems="10"
        android:inputType="textPersonName"
        android:id="@+id/sign_up_street" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView8"
        android:layout_below="@+id/textView8"
        android:layout_marginTop="12dp"
        android:text="State" />

    <EditText
        android:id="@+id/sign_up_state"
        android:layout_width="55dp"
        android:layout_height="20dp"
        android:layout_alignBottom="@+id/textView9"
        android:layout_alignStart="@+id/sign_up_button"
        android:ems="10"
        android:inputType="textPersonName" />

    <TextView
        android:id="@+id/textViewofZipCode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/sign_up_state"
        android:layout_toEndOf="@+id/sign_up_button"
        android:text="ZipCode" />

    <EditText
        android:id="@+id/editText4"
        android:layout_width="55dp"
        android:layout_height="15dp"
        android:ems="10"
        android:inputType="number"
        android:layout_alignBottom="@+id/textView12"
        android:layout_alignEnd="@+id/sign_up_street" />

    <EditText
        android:id="@+id/signup_View_ZipCode"
        android:layout_width="55dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/sign_up_street"
        android:ems="10"
        android:inputType="textPersonName"
        android:layout_alignBottom="@+id/sign_up_state"
        android:layout_alignEnd="@+id/sign_up_street" />


</RelativeLayout>