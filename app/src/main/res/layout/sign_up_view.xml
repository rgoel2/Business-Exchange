<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/sign_up_password"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/signup_first_name_text_view"
        android:layout_width="75dp"
        android:layout_height="25dp"
        android:layout_marginLeft="29dp"
        android:layout_marginStart="31dp"
        android:text="FirstName "
        android:layout_marginTop="26dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:id="@+id/sign_up_first_name_edit_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/signup_first_name_text_view"
        android:layout_marginLeft="27dp"
        android:layout_marginStart="27dp"
        android:layout_toEndOf="@+id/signup_first_name_text_view"
        android:layout_toRightOf="@+id/signup_first_name_text_view"
        android:ems="10"
        android:inputType="textPersonName" />

    <TextView
        android:id="@+id/signup_last_name_text_view"
        android:layout_width="75dp"
        android:layout_height="25dp"
        android:layout_alignLeft="@+id/signup_first_name_text_view"
        android:layout_alignStart="@+id/signup_first_name_text_view"
        android:layout_below="@+id/sign_up_first_name_edit_text"
        android:layout_marginTop="33dp"
        android:layout_toLeftOf="@+id/sign_up_first_name_edit_text"
        android:layout_toStartOf="@+id/sign_up_first_name_edit_text"
        android:text="LastName" />

    <TextView
        android:id="@+id/signup_profession_text_view"
        android:layout_width="75dp"
        android:layout_height="25dp"
        android:layout_alignLeft="@+id/signup_last_name_text_view"
        android:layout_alignStart="@+id/signup_last_name_text_view"
        android:layout_below="@+id/signup_last_name_text_view"
        android:layout_marginTop="29dp"
        android:text="Profession" />

    <TextView
        android:id="@+id/signup_city_text_view"
        android:layout_width="75dp"
        android:layout_height="25dp"
        android:layout_alignEnd="@+id/signup_profession_text_view"
        android:layout_alignLeft="@+id/signup_profession_text_view"
        android:layout_alignRight="@+id/signup_profession_text_view"
        android:layout_alignStart="@+id/signup_profession_text_view"
        android:layout_below="@+id/signup_profession_text_view"
        android:layout_marginTop="33dp"
        android:text="City" />

    <TextView
        android:id="@+id/signup_email_text_view"
        android:layout_width="75dp"
        android:layout_height="35dp"
        android:layout_alignLeft="@+id/signup_city_text_view"
        android:layout_alignStart="@+id/signup_city_text_view"
        android:layout_below="@+id/signup_city_text_view"
        android:layout_marginTop="27dp"
        android:text="Email Address" />

    <EditText
        android:id="@+id/sign_up_last_name_edit_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/signup_profession_text_view"
        android:layout_alignLeft="@+id/sign_up_first_name_edit_text"
        android:layout_alignStart="@+id/sign_up_first_name_edit_text"
        android:ems="10"
        android:inputType="textNoSuggestions|textPersonName" />

    <EditText
        android:id="@+id/sign_up_profession_edit_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/signup_city_text_view"
        android:layout_alignLeft="@+id/sign_up_last_name_edit_text"
        android:layout_alignStart="@+id/sign_up_last_name_edit_text"
        android:ems="10"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/sign_up_city_edit_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/signup_email_text_view"
        android:layout_alignLeft="@+id/sign_up_profession_edit_text"
        android:layout_alignStart="@+id/sign_up_profession_edit_text"
        android:ems="10"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/sign_up_email_edit_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/signup_email_text_view"
        android:layout_alignLeft="@+id/sign_up_city_edit_text"
        android:layout_alignStart="@+id/sign_up_city_edit_text"
        android:ems="10"
        android:inputType="textEmailAddress" />

    <TextView
        android:id="@+id/signup_image_text_view"
        android:layout_width="75dp"
        android:layout_height="35dp"
        android:layout_alignLeft="@+id/signup_email_text_view"
        android:layout_alignStart="@+id/signup_email_text_view"
        android:layout_below="@+id/signup_email_text_view"
        android:layout_marginTop="21dp"
        android:text="Upload Picture" />

    <Button
        android:id="@+id/sign_up_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:onClick="addUser"
        android:text="Sign Up" />

    <TextView
        android:id="@+id/signup_contact_text_view"
        android:layout_width="75dp"
        android:layout_height="25dp"
        android:layout_alignLeft="@+id/signup_image_text_view"
        android:layout_alignStart="@+id/signup_image_text_view"
        android:layout_below="@+id/signup_image_text_view"
        android:layout_marginTop="31dp"
        android:text="Contact" />

    <EditText
        android:id="@+id/sign_up_contact_edit_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/signup_contact_text_view"
        android:layout_alignStart="@+id/sign_up_take_picture"
        android:ems="10"
        android:inputType="number" />

    <ImageButton
        android:id="@+id/sign_up_take_picture"
        android:layout_width="125dp"
        android:layout_height="35dp"
        android:layout_above="@+id/signup_contact_text_view"
        android:layout_alignEnd="@+id/sign_up_email_edit_text"
        android:layout_alignStart="@+id/sign_up_email_edit_text"
        android:layout_alignTop="@+id/signup_image_text_view"
        android:onClick="openCamera"
        app:srcCompat="@drawable/images" />

    <TextView
        android:id="@+id/signup_street_text_view"
        android:layout_width="75dp"
        android:layout_height="25dp"
        android:layout_alignStart="@+id/signup_contact_text_view"
        android:layout_below="@+id/signup_contact_text_view"
        android:layout_marginTop="12dp"
        android:text="Street" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/signup_street_text_view"
        android:layout_alignStart="@+id/sign_up_contact_edit_text"
        android:layout_below="@+id/sign_up_contact_edit_text"
        android:ems="10"
        android:inputType="textPersonName"
        android:id="@+id/sign_up_street_edit_text" />

    <TextView
        android:id="@+id/signup_state_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/signup_street_text_view"
        android:layout_below="@+id/signup_street_text_view"
        android:layout_marginTop="12dp"
        android:text="State" />

    <EditText
        android:id="@+id/sign_up_state_edit_text"
        android:layout_width="55dp"
        android:layout_height="20dp"
        android:layout_alignBottom="@+id/signup_state_text_view"
        android:layout_alignStart="@+id/sign_up_button"
        android:ems="10"
        android:inputType="textPersonName" />

    <TextView
        android:id="@+id/signup_zipcode_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/sign_up_state_edit_text"
        android:layout_toEndOf="@+id/sign_up_button"
        android:text="ZipCode" />

    <EditText
        android:id="@+id/signup_zipcode_edit_text"
        android:layout_width="55dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/sign_up_state_edit_text"
        android:layout_alignEnd="@+id/sign_up_street_edit_text"
        android:layout_below="@+id/sign_up_street_edit_text"
        android:ems="10"
        android:inputType="textPersonName" />


</RelativeLayout>